    <path pathString="EmbeddedPy" checkConnect="true" sfe="false" seoe="false">
      <scriptName>EmbeddedScript</scriptName>
      <scriptText><![CDATA[from org.csstudio.opibuilder.scriptUtil import PVUtil

_longitude = PVUtil.getDouble(pvs[0])
_hemisphere = "L"

if (_longitude < 0):
	_hemisphere = "W"

_degree = int(_longitude)
_minutes_f = (_degree - _longitude) * 60
_minutes = int (_minutes_f)
_seconds = (_minutes_f - _minutes)*60

pvs[1].setValue("%io %2i ' %.3f '' %s" % (abs(_degree), _minutes, _seconds, _hemisphere))

	]]></scriptText>
      <pv trig="true">Cnt:$(receiver_id):GPS:Longitude</pv>
