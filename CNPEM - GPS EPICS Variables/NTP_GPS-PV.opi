    <path pathString="EmbeddedPy" checkConnect="true" sfe="false" seoe="false">
      <scriptName>EmbeddedScript</scriptName>
      <scriptText><![CDATA[from org.csstudio.opibuilder.scriptUtil import PVUtil

_latitude = PVUtil.getDouble(pvs[0])
_hemisphere = "N"

if (_latitude < 0):
	_hemisphere = "S"

_degree = int(_latitude)
_minutes_f = (_degree - _latitude) * 60
_minutes = int (_minutes_f)
_seconds = (_minutes_f - _minutes)*60

pvs[1].setValue("%io %i ' %.3f '' %s" % (abs(_degree), _minutes, _seconds, _hemisphere))]]></scriptText>
      <pv trig="true">Cnt:$(receiver_id):GPS:Latitude</pv>
      <pv trig="false">loc://latitude</pv>
